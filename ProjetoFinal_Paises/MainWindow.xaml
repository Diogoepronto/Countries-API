<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:syncfusion="http://schemas.syncfusion.com/wpf" x:Class="ProjetoFinal_Paises.MainWindow"
    mc:Ignorable="d"
    Title="MainWindow" Height="800" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="20" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="20" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="20" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="20" />
            <RowDefinition Height="100*" />
            <RowDefinition Height="23*" />
            <RowDefinition Height="621*" />
            <RowDefinition Height="20" />
        </Grid.RowDefinitions>
        <ListBox x:Name="listBoxTeste" Grid.Column="1" Grid.Row="1" Grid.RowSpan="3" />

        <syncfusion:ComboBoxAdv x:Name="comboBoxAdv"
                                AllowMultiSelect="True"
                                Grid.Column="3"
                                DefaultText="ComboBox Items" Grid.Row="1" Margin="95,28,125,48">
            <syncfusion:ComboBoxItemAdv Content="Item 1" />
            <syncfusion:ComboBoxItemAdv Content="Item 2" />
            <syncfusion:ComboBoxItemAdv Content="Item 3" />
            <syncfusion:ComboBoxItemAdv Content="Item 4" />
            <syncfusion:ComboBoxItemAdv Content="Item 5" />
        </syncfusion:ComboBoxAdv>

    </Grid>
</Window>